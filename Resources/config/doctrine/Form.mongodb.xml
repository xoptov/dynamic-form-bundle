<doctrine-mongo-mapping xmlns="http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping http://doctrine-project.org/schemas/odm/doctrine-mongo-mapping.xsd">
    <mapped-superclass name="Xoptov\DynamicFormBundle\Document\Form">
        <field name="id" id="true" type="string"/>
        <field name="name" type="string"/>
        <embed-many field="fields" target-document="Xoptov\DynamicFormBundle\Model\FieldInterface"/>
    </mapped-superclass>
</doctrine-mongo-mapping>